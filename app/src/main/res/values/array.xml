<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="time_spinner">
        <item>小时</item>
        <item>分钟</item>
        <item>次</item>
        <item>自定义</item>
    </string-array>

    <string-array name="time_spinner_two">
        <item>每天</item>
        <item>每周</item>
        <item>每月</item>
    </string-array>

<!--    MyTextView-->
    <declare-styleable name="DeleteLineTextView">
        <attr name="showDeleteLine">
            <enum name="show" value="0" />
            <enum name="hide" value="1" />
        </attr>
        <attr name="deleteLineColor" format="color|reference" />
        <attr name="deleteLineWidth" format="dimension" />
    </declare-styleable>
</resources>
